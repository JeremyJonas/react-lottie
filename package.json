{
  "name": "react-lottie",
  "version": "1.1.0",
  "description": "lottie animation view for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenqingspring/react-lottie"
  },
  "license": "MIT",
  "scripts": {
    "release": "git add package.json && git commit -m 'chore: bump version number' && standard-version --first-release && git push --follow-tags origin master && npm publish",
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src stories test",
    "lintfix": "eslint src --fix",
    "testonly": "jest",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch",
    "start": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "postpublish": "npm run publish-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-knobs": "^3.4.3",
    "@storybook/addon-options": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "git-url-parse": "^9.0.0",
    "jest": "^22.4.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sinon": "^5.0.3",
    "storybook-readme": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0"
  },
  "dependencies": {
    "lottie-web": "^5.1.3"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  }
}
